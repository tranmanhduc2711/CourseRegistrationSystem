<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sample.Classinfo" table="classinfo" schema="portal">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="classId">
            <column name="class_id" sql-type="char(10)" length="10" not-null="true"/>
        </property>
        <property name="userId">
            <column name="user_id" sql-type="char(8)" length="8" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>